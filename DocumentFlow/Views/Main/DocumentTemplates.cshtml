@model IEnumerable<EntityModels.DocumentTemplate>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table style="margin-bottom: 20px; width: 40%;">
    @if (Model != null && Model.Count() != 0)
    {
        <thead>
        <tr>
            <th>Название</th>
            <th></th>
        </tr>
        </thead>
        foreach (var item in Model.AsEnumerable())
        {
            <tbody>
            <tr>
                <td>@item.Name</td>
                <td>@Html.ActionLink("Заполнить", "FillDocument", "Main", new {item.Id}, null)</td>
            </tr>
            </tbody>
        }
    }
    else
    {
        <tr>список пуст</tr>
    }
</table>